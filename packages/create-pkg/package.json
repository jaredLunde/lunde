{
  "name": "@lunde/create-pkg",
  "version": "1.0.55",
  "main": "index.js",
  "author": "Jared Lunde <jared@BeStellar.co> (https://jaredLunde.com)",
  "license": "MIT",
  "description": "An @inst-pkg template for creating new TypeScript packages",
  "dependencies": {
    "@inst-cli/template-utils": "^1.0.0"
  },
  "repository": "https://github.com/jaredLunde/lunde/tree/master/packages/create-pkg",
  "homepage": "https://github.com/jaredLunde/lunde/tree/master/packages/create-pkg#readme",
  "bin": {
    "create-pkg": "bin/index.js"
  },
  "files": [
    "index.js",
    "/lib",
    "/bin"
  ],
  "scripts": {
    "format": "npm run format:src && npm run format:lib",
    "format:src": "prettier --write \"./*.{js,md}\"",
    "format:lib": "prettier --write \"./lib/**/*.{yml,js,ts,md}\"",
    "prepublishOnly": "npm run format"
  },
  "devDependencies": {
    "prettier": "^1.18.2"
  }
}
